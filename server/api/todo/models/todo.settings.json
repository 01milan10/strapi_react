{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "name": "todo"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "isCompleted": {
      "type": "boolean",
      "default": false
    }
  }
}
